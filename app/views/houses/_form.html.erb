<%= form_with(model: @user, local: true) do |f| %>

    <%= f.label :物件名 %><br>
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.label :賃料 %><br>
    <%= f.text_field :rent %>円
    <br>
    <br>
    <%= f.label :住所 %><br>
    <%= f.text_field :address %>
    <br>
    <br>
    <%= f.label :築年数 %><br>
    <%= f.text_field :age %>年
    <br>
    <br>
    <%= f.label :備考 %><br>
    <%= f.text_area :remarks %>
    <br>

    <h2>最寄り駅1</h2>

    <%= f.fields_for :stations do | s | %>

        <%= s.label :路線名 %><br>
        <%= s.text_field :route %>
        <br>
        <br>
        <%= s.label :駅名 %><br>
        <%= s.text_field :station_name %>
        <br>
        <br>
        <%= s.label :徒歩分数 %><br>
        <%= s.text_field :minites %>分
        <br>
        <br>
        
    <% end %>
    <%= f.submit "登録する" %>
<% end %>

